{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "envMode": "loose",
  "tasks": {
    "dev": {
      "persistent": true,
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "test": {
      "persistent": true
    },
    "db:generate:incentives": {
      "cache": false,
      "persistent": true
    },
    "db:generate:consultation": {
      "cache": false,
      "persistent": true
    },
    "db:migrate:incentives": {
      "cache": false,
      "persistent": true
    },
    "db:migrate:consultation": {
      "cache": false,
      "persistent": true
    },
    "db:studio:incentives": {
      "cache": false
    },
    "db:studio:consultation": {
      "cache": false
    },
    "db:push:incentives": {
      "cache": false,
      "persistent": true
    }
  }
}
